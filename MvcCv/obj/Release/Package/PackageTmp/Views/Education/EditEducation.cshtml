@model MvcCv.Models.Entity.TblEducation
@{
    ViewBag.Title = "EditEducation";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Edit Education</h2>

<form method="post">
    @Html.Label("Header")
    @Html.TextBoxFor(x => x.Header, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.Header, "", new { @style = "color:red" })
    <br />
    @Html.Label("Subheader1")
    @Html.TextBoxFor(x => x.Subheader1, new { @class = "form-control" })
    <br />
    @Html.Label("Subheader2")
    @Html.TextBoxFor(x => x.Subheader1, new { @class = "form-control" })
    <br />
    @Html.Label("GNO")
    @Html.TextBoxFor(x => x.GNO, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.GNO, "", new { @style = "color:red" })

    <br />
    @Html.Label("Date")
    @Html.TextBoxFor(x => x.Subheader1, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Save</button>
</form>
