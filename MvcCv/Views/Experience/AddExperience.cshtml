@model MvcCv.Models.Entity.TblExperience
@{
    ViewBag.Title = "AddExperience";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Add Experience</h2>

<form method="POST">
    <input type="text" name="Header" class="form-control" placeholder="Header" />
    <br>
    <input type="text" name="Subheader" class="form-control" placeholder="Subheader" />
    <br>
    <input type="text" name="Date" class="form-control" placeholder="Date" />
    <br>
    <textarea placeholder="Description" name="Description" class="form-control" rows="5"></textarea>
    <br>
    <button class="btn btn-info">Kaydet</button>
</form>
